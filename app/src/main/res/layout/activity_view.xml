<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".menu.ViewActivity">
    <TextView
            android:text="Workout Description"
            android:layout_width="128dp"
            android:layout_height="23dp"
            android:id="@+id/workoutTitleDescription" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="88dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:text="Workout Tiitle"
            android:layout_width="128dp"
            android:layout_height="23dp"
            android:id="@+id/workoutTitleView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <Button
            android:text="Start"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:id="@+id/button2" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="12dp"
            android:layout_marginTop="96dp" app:layout_constraintTop_toBottomOf="@+id/workoutTitleDescription"/>
    <Button
            android:text="Delete"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:id="@+id/button5" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="164dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/workoutTitleDescription"
            app:layout_constraintHorizontal_bias="0.592"/>
</android.support.constraint.ConstraintLayout>